<?xml version="1.0"?>
<rdf:RDF xmlns="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#"
     xml:base="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:beAWARE_ontology="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology">
        <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#causedByDisaster -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#causedByDisaster">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#causesDisasterImpact"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#ImpactType"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#NaturalDisasterType"/>
        <rdfs:label>caused by disaster</rdfs:label>
        <skos:definition>Indicates which disaster type is responsible for this impact type.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#causedByIncident -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#causedByIncident">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#causesIncidentImpact"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#ImpactType"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#IncidentType"/>
        <rdfs:label>caused by incident</rdfs:label>
        <skos:definition>Indicates which incident type has caused this impact type.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#causesDisasterImpact -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#causesDisasterImpact">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#NaturalDisasterType"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#ImpactType"/>
        <rdfs:label>causes disaster impact</rdfs:label>
        <skos:definition>Indicates which impact type is caused by this natural disaster type.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#causesIncidentImpact -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#causesIncidentImpact">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#IncidentType"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#ImpactType"/>
        <rdfs:label>causes incident impact</rdfs:label>
        <skos:definition>Indicates which impact type is caused by this type of incident.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#characterizedByParameterType -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#characterizedByParameterType">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#characterizesDisasterType"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#NaturalDisasterType"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#ClimateParameterType"/>
        <rdfs:label>characterized by parameter type</rdfs:label>
        <skos:definition>Associates a natural disaster type with a climate parameter type.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#characterizesDisasterType -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#characterizesDisasterType">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#ClimateParameterType"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#NaturalDisasterType"/>
        <rdfs:label>characterizes disaster type</rdfs:label>
        <skos:definition>Associates a climate parameter type with a natural disaster type.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#containedInDataset -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#containedInDataset">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#containsDetection"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Detection"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Dataset"/>
        <rdfs:label>contained in dataset</rdfs:label>
        <skos:definition>Indicates the dataset this detection object is contained in.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#containsDetection -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#containsDetection">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Dataset"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Detection"/>
        <rdfs:label>contains detection</rdfs:label>
        <skos:definition>A dataset may contain multiple detections.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#derivedFrom -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#derivedFrom">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Incident"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Incident"/>
        <rdfs:label>derived from</rdfs:label>
        <skos:definition>Represents the fact that an incident may lead to other incidents.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#detectsIncident -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#detectsIncident">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#incidentDetectedBy"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Detection"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Incident"/>
        <rdfs:label>detects incident</rdfs:label>
        <skos:definition>Indicates which incidents were detected by a specific detection activity.</skos:definition>
        <skos:example>A flooding incident, a blocked road etc.</skos:example>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#detectsParticipant -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#detectsParticipant">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#participantDetectedBy"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Detection"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#VulnerableObject"/>
        <rdfs:label>detects participant</rdfs:label>
        <skos:definition>Indicates which participants were detected by a specific detection activity.</skos:definition>
        <skos:example>Humans in danger, submerged cars etc.</skos:example>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasClimateParameterMeasurement -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasClimateParameterMeasurement">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#relatesToNaturalDisaster"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#NaturalDisaster"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#ClimateParameter"/>
        <rdfs:label>has disaster climate parameter</rdfs:label>
        <skos:definition>Associates a natural disaster occurrence to a climate parameter measurement.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasClimateParameterOccurrence -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasClimateParameterOccurrence">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#isOfClimateParameterType"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#ClimateParameterType"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#ClimateParameter"/>
        <rdfs:label>has climate parameter occurrence</rdfs:label>
        <skos:definition>Assigns a climate parameter type to a specific climate parameter.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasDisasterLocation -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasDisasterLocation">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#isLocationOfDisaster"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#NaturalDisaster"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Location"/>
        <rdfs:label>has disaster location</rdfs:label>
        <skos:definition>Represents the location where a natural disaster takes place.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasDisasterOccurrence -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasDisasterOccurrence">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#isOfDisasterType"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#NaturalDisasterType"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#NaturalDisaster"/>
        <rdfs:label>has disaster occurrence</rdfs:label>
        <skos:definition>Links a natural disaster type to a natural disaster.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasImpactOccurrence -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasImpactOccurrence">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#isOfImpactType"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#ImpactType"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Impact"/>
        <rdfs:label>has impact occurrence</rdfs:label>
        <skos:definition>Links an impact type to a specific impact pccurrence.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasImpactOn -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasImpactOn">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#isImpactedBy"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Impact"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#VulnerableObject"/>
        <rdfs:label>has impact on</rdfs:label>
        <skos:definition>Indicates which vulnerable object is impacted by the specific impact occurrence.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasIncidentClimateParameter -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasIncidentClimateParameter">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#isClimateParameterOfIncident"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Incident"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#ClimateParameter"/>
        <rdfs:label>has incident climate parameter</rdfs:label>
        <skos:definition>Associates an incident to a climate parameter measurement.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasIncidentImpact -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasIncidentImpact">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#isImpactOfIncident"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Incident"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Impact"/>
        <rdfs:label>has incident impact</rdfs:label>
        <skos:definition>Indicates the impact of the specific incident.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasIncidentLocation -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasIncidentLocation">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#isLocationOfIncident"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Incident"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Location"/>
        <rdfs:label>has incident location</rdfs:label>
        <skos:definition>Represents the location where an incident occurs.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasIncidentOccurrence -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasIncidentOccurrence">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#isOfIncidentType"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#IncidentType"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Incident"/>
        <rdfs:label>has incident occurrence</rdfs:label>
        <skos:definition>Links an incident type to an incident.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasMeasurementLocation -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasMeasurementLocation">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#isLocationOfMeasurement"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#ClimateParameter"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Location"/>
        <rdfs:label>has measurement location</rdfs:label>
        <skos:definition>Represents the location where a climate parameter measurement was taken.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasMediaLocation -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasMediaLocation">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#isLocationOfMediaItem"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#MediaItem"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Location"/>
        <rdfs:label>has media location</rdfs:label>
        <skos:definition>Represents the location where a media item was captured.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasReportLocation -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasReportLocation">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#isLocationOfReport"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#IncidentReport"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Location"/>
        <rdfs:label>has report location</rdfs:label>
        <skos:definition>Indicates the location of the incident report.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasResponderLocation -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#hasResponderLocation">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#isLocationOfResponder"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Responder"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Location"/>
        <rdfs:label>has responder location</rdfs:label>
        <skos:definition>Represents the location where a rescue unit is at.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#incidentDetectedBy -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#incidentDetectedBy">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Incident"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Detection"/>
        <rdfs:label>incident detected by</rdfs:label>
        <skos:definition>Indicates the detection activity that detected this incident.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#involvedInDataset -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#involvedInDataset">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#involvesIncident"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Incident"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Dataset"/>
        <rdfs:label>involved in dataset</rdfs:label>
        <skos:definition>Indicates the dataset this incident is involved in.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#involvedInIncident -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#involvedInIncident">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#involvesParticipant"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#VulnerableObject"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/beAWARE-project/ontology/master/beAWARE_ontology#Incident"/>
        <rdfs:label>involved in incident</rdfs:label>
        <skos:definition>Indicates the incident a vulnerable object is involved in.</skos:definition>
    </owl:ObjectProperty>
    


